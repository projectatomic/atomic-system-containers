			"destination": "/proc",
			"type": "proc",
			"source": "proc"
		},
		{
			"type": "bind",
			"source": "/dev",
			"destination": "/dev",
			"options": [
				"rbind",
				"rw",
				"mode=755"
			]
		},
		{
			"destination": "/sys",
			"type": "bind",
			"source": "/sys",
			"options": [
				"rbind",
				"rw",
				"mode=755"
			]
		},
		{
			"type": "bind",
			"source": "/etc/firewalld",
			"destination": "/etc/firewalld",
			"options": [
				"rbind",
				"rw",
				"mode=755"
			]
		},
		{
			"type": "bind",
			"source": "/etc/sysconfig",
			"destination": "/etc/sysconfig",
			"options": [
				"rbind",
				"rw",
				"mode=755"
			]
		},
		{
			"destination": "/etc/resolv.conf",
			"type": "bind",
			"source": "/etc/resolv.conf",
			"options": [
				"ro",
				"rbind",
				"rprivate"
			]
		},
		{
			"type": "bind",
			"source": "${RUN_DIRECTORY}/dbus/system_bus_socket",
			"destination": "/var/run/dbus/system_bus_socket",
			"options": [
				"ro",
				"rbind",
				"rprivate"
			]
		},
		{
			"type": "bind",
			"source": "${RUN_DIRECTORY}/${NAME}",
			"destination": "/run/firewalld",
			"options": [
				"rbind",
				"rw",
				"mode=755"
			]
		},
		{
			"type": "bind",
			"source": "/var/log/firewalld",
			"destination": "/var/log/firewalld",
			"options": [
